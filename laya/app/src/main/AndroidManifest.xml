<?xml version="1.0" encoding="utf-8"?><manifest xmlns:android="http://schemas.android.com/apk/res/android"
package="com.abheri.laya">

<!-- <uses-permission android:name="android.permission.READ_PHONE_STATE"/> -->

<uses-permission android:name="android.permission.INTERNET" />
<uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
<uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
<uses-permission android:name="com.android.vending.BILLING" />

<application
    android:name=".activities.CheckoutApplication"
    android:allowBackup="true"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
        android:name=".activities.HomeActivity"
        android:screenOrientation="portrait">
        <intent-filter>
            <action android:name="android.intent.action.MAIN" />

            <category android:name="android.intent.category.LAUNCHER" />
        </intent-filter>
    </activity>

    <activity android:name=".activities.StaticActivity"
        android:label="@string/use_case_title_static"
        android:parentActivityName=".activities.MainActivity"
        xmlns:tools="http://schemas.android.com/tools"
        tools:ignore="UnusedAttribute">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".activities.MainActivity" />
    </activity>

    <activity android:name=".activities.SkusActivity"
        android:label="@string/use_case_title_skus"
        android:parentActivityName=".activities.MainActivity"
        xmlns:tools="http://schemas.android.com/tools"
        tools:ignore="UnusedAttribute">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".activities.MainActivity" />
    </activity>
    <activity
        android:name=".activities.SubscriptionsActivity"
        android:label="@string/use_case_title_subscriptions"
        android:parentActivityName=".activities.MainActivity"
        xmlns:tools="http://schemas.android.com/tools"
        tools:ignore="UnusedAttribute">
        <meta-data
            android:name="android.support.PARENT_ACTIVITY"
            android:value=".activities.MainActivity" />
    </activity>

    <!--
        <receiver android:name=".util.EventBroadcastReceiver" >
            <intent-filter>
                <action android:name="android.intent.action.PHONE_STATE" >
                </action>
            </intent-filter>
        </receiver>
        -->
    <activity android:name=".activities.SignupActivity" />
    <activity android:name=".activities.LoginActivity" />
    <activity android:name=".activities.MainActivity" />
</application>

</manifest>